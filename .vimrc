"""""GENERAL""""""

"""syntax
syntax on

"""settings
set nocompatible
set autoread
set showmatch
set linespace=0
set wildmenu
set history=100
set encoding=utf-8
set showcmd

"""""EDIT"""""
set autoindent
set smartindent
set expandtab
set smarttab
set tabstop=4
set shiftround
set shiftwidth=4

"""""COLOR""""""

colorscheme desert

hi Pmenu      ctermfg=Black ctermbg=Grey
hi PmenuSel   ctermbg=Blue
hi PmenuSbar  ctermbg=Cyan


"""""SEARCH""""""
set incsearch
set ignorecase
set wrapscan
set smartcase
